def modelReviseBJ(mdl, vtype=''):
	model = mdl.upper()
	ml = len(model)
	if ml < 7:
		return model
	#print '--- begin revise model %s ---' % model
	if model[0:3] == '13J': # Normal:  BJ[1345]\d{3}[A-Z]{5}-?[0-9A-Z]*  4 + 5 - 1/2  Main + Tech + '-' + Suffix (2)
		model = model.replace(model[:3],'BJ')
		ml -= 1
	elif model[0:2] == '8J':
		model = model.replace(model[:2],'BJ')
	elif model[0:2] == '5J':
		model = model.replace(model[:2],'BJ')
