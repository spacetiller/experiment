			elif model.find("CLXY") > 0 and len(model) > 12 :
				dc = model[11:12].upper()
		elif model[2:3] == '5' :
			return drive
		#elif model[2:3] == '5' :
		#	if len(model) > 13 and model[5:6].isdigit() == True and model[6:7].isdigit() == False and model[6:9] in special_code_dict:
	if dc == 'A':
